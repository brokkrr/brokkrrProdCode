    }

        button[type='submit']:hover {
            cursor: pointer;
        }

            button[type='submit']:hover #i {
                color: #fff;
            }

    .friend {
        float: left;
        /*margin: 30px 0 0 0px;*/
    }

    .me {
        float: right;
        /*margin: 20px 15px 0 0;*/
    }

