        };

        var myLatlng1 = new google.maps.LatLng(33.440344, -117.583902)

        //var myLatlng1 = new google.maps.LatLng(18.508920,73.926026)

        function initialize(strtitle, lat, lon) {
            var myLatlng = new google.maps.LatLng(lat, lon) // This is used to center the map to show our markers
            var mapOptions = {
                center: myLatlng1,
                zoom: 9,
                mapTypeId: google.maps.MapTypeId.HYBRID,
                marker: true
                //ROADMAP
            };
            // var infoWindow = new google.maps.InfoWindow();
            var map = new google.maps.Map(document.getElementById("googleMap"), mapOptions);

            var marker = new google.maps.Marker({
                map: map,
                position: myLatlng,
                icon: "../../Images/Assets/location.png",
                title: strtitle

            });

            var boxText = document.createElement("div");
            boxText.style.cssText = "padding-left:20px;padding-top:10px; height:auto;font-size:14px;border-radius:10px;";
            boxText.innerHTML = '<strong style="color:darkturquoise">Brokkrr Inc.</strong><br><style="color:#939393">232 Avenida Fabricante <br>#105 San Clemente <br>CA 92672<br><img src="/Images/Assets/call.png" /> (310) 657-4315 <br><img src="/Images/Assets/email.png" /><a href="mailto:info@brokkrr.com" style="color:#939393"> info@brokkrr.com';


            var myOptions = {
                content: boxText
                , disableAutoPan: false
                , maxWidth: 0
                , pixelOffset: new google.maps.Size(10, -160)
                , zIndex: null
                , boxStyle: {
                    opacity: 0.80
                  , width: "180px"
                  , height: "140px"
                  , background: "url('../../Images/Assets/MapArrow.png') no-repeat"


                }
                , closeBoxMargin: "10px 2px 2px 2px"
                , closeBoxURL: ""
                , infoBoxClearance: new google.maps.Size(1, 1)
                , isHidden: false
                , pane: "floatPane"
                , enableEventPropagation: false
            };

            var ib = new InfoBox(myOptions);
            ib.open(map, marker);

        }


    </script>



