    $(function () {
        $('#chkSubIndustry1').multiselect({
            maxHeight: 300,
            buttonWidth: '190px',

            onChange: function (element, checked) {
                if (checked == true) {
                    var i = 0;
                    $("select#chkSubIndustry1 option:selected").each(function () {
                        i = i + 1;
                        if (i > 5) {
                            $("#divmain").removeClass("addScroll");
                            $("#divmain").addClass("removeScroll");
                            $("#lblError").text('Sorry! Select max. 5 NAICS codes.');
                            $("#ModalError").modal();
                            $('#chkSubIndustry1').multiselect('deselect', $(this).val());
                        }
                    });
                }
            }
        });
    });

