<script type="text/javascript">
    $(document).ready(function () {
        $("#SubmitInfo").bind("click", function () {

            var FirstName = $("#FirstName").val();
            var PhoneNo = $("#PhoneNo").val();
            var EmailId = $("#EmailId").val();
            var Password = $("#Password").val();
            var ConfirmPassword = $("#ConfirmPassword").val();
            var Area = $("#Area").val();
            var Zip = $("#Zip").val();

            if (FirstName != '' && PhoneNo != '' && EmailId != '' && Password != '' && ConfirmPassword != ''
                && Area != '' && Zip != '' && (Password == ConfirmPassword) && (Password.length >= 8) && Zip.length == 5) {
                $('#loading').show();
                $('#myPage').addClass("bodyblur");
                $('#priorities').addClass("bodyblur");
                $('#AreaContent').addClass("bodyblur");
            }
            else {
                $('#loading').hide();
                $('#myPage').removeClass("bodyblur");
                $('#priorities').removeClass("bodyblur");
                $('#AreaContent').removeClass("bodyblur");

                //$('#loading').show();
                //$('#myPage').addClass("bodyblur");
                //$('#priorities').addClass("bodyblur");
                //$('#AreaContent').addClass("bodyblur");
            }
        });
    });

