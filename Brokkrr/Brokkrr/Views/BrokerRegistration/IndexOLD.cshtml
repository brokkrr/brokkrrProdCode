                }
            }
            else if (this.files[0].size / 1024 <= 1024 && this.files[0].size / 1024 > 0) {
                var FR = new FileReader();
                FR.readAsDataURL(this.files[0]);
                FR.onload = function (e) {
                    var base64 = e.target.result;
                    base64 = base64.replace(/^data:image\/(png|jpg|jpeg|gif);base64,/, "").replace(/^data:;base64,/, "").replace(/^data:text\/html;base64,/, "");
                    $("#HiddenCmpLogo" + value).val(base64);
                    $("#CmpLogo" + value).val(FileName);
                    $("#fileToUploadExp" + value).val('');
                }
            }
            else {
                $("#HiddenCmpLogo" + value).val('');
                $("#CmpLogo" + value).val('');
                $("#fileToUploadExp" + value).val('');
                $("#divmain").removeClass("addScroll");
                $("#divmain").addClass("removeScroll");
                $("#lblError").text('File Size Should not greater than 4 MB.');
                $("#ModalError").modal();
            }
        });
    }

</script>

