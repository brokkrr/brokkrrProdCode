<script type="text/javascript">
    $(document).ready(function () {

        //$("#notificationbar").css("display", "block");
        $(".notification").css("display", "block");
        $("#aSerchBrokerMeineke").css("display", "none");
        $("#aSerchBrokerBrokkrr").css("display", "block");

        var Auth = '@ViewBag.User';
        var Comp = '@ViewBag.Company';

        if (Auth == "Authorize") {
            if (Comp == "Meineke") {
                //alert("Auth Meineke");
                $("#MeinekeHomePageImage").css("display", "block");
                $("#HomePageImage").css("display", "none");

                $("#MenuMeinekeHomePage").css("display", "block");
                $("#MenuHomePage").css("display", "none");
                $("#MenuMeinekeCustomerProfile").css("display", "block");
                $("#MenuCustomerProfile").css("display", "none");

                $("#footerBrokerLogo").css("display", "none");

                $("#footerLayout1").css("display", "none");

                $("#meinekefotter").css("display", "block");
                $("#APSPfooter").css("display", "none");

            }
            else if (Comp == "Brokkrr") {
                //alert("Auth Brokkrr");
                $("#MeinekeHomePageImage").css("display", "none");
                $("#HomePageImage").css("display", "block");

                $("#MenuMeinekeHomePage").css("display", "none");
                $("#MenuHomePage").css("display", "block");
                $("#MenuMeinekeCustomerProfile").css("display", "none");
                $("#MenuCustomerProfile").css("display", "block");

                $("#footerBrokerLogo").css("display", "block");

                $("#footerLayout1").css("display", "block");

                $("#meinekefotter").css("display", "none");
                $("#APSPfooter").css("display", "none");

            }
            else if (Comp == "APSP")
            {
                $("#APSPHomePageImage").css("display", "block");
                $("#MenuAPSPHomePage").css("display", "block");
                $("#MenuAPSPCustomerProfile").css("display", "block");
                $("#aSerchBrokerAPSP").css("display", "block");


                $("#footerLayout1").css("display", "none");

                $("#meinekefotter").css("display", "none");
                $("#APSPfooter").css("display", "block");
            }
        }
        else {
            if (Comp == "Meineke") {
                //alert("UnAuth Meineke");
            }
            else {
                //alert("UnAuth Brokkrr");
            }
        }

        var ProfilePic = '@ViewBag.ProfilePic';

        if (ProfilePic != "") {
            var Time = '@ViewBag.Time';
            $('#profilepicprofile').attr("src", ProfilePic + "?" + Time);
            $('#profilepicprofile').css('display', 'block');
            $('#profileImage1').css('display', 'none');
        }
        else {
            var ProfilePic = '@ViewBag.Initials';
            $('#profileImage1').text(ProfilePic);
            $('#profileImage1').css('display', 'block');
        }

        var DetailsSaved = '@ViewBag.DetailsSaved';

        if (DetailsSaved != "") {
            $("#lblSuccess").text('Details saved successfully.');
            $("#ModalError").modal('show');
        }

        $("#FirstName").keyup(function (e) {
            var FirstName = $("#FirstName").val();
            if (FirstName != "") {
                $("#hrFirstName").removeClass("hrRequired");
                $("#hrFirstName").addClass("hr");
            }
            else {
                $("#hrFirstName").removeClass("hr");
                $("#hrFirstName").addClass("hrRequired");
            }
        });

        $("#LastName").keyup(function (e) {
            var LastName = $("#LastName").val();
            if (LastName != "") {
                $("#hrLastName").removeClass("hrRequired");
                $("#hrLastName").addClass("hr");
            }
            else {
                $("#hrLastName").removeClass("hr");
                $("#hrLastName").addClass("hrRequired");
            }
        });

        $("#PhoneNo").keyup(function (e) {
            var PhoneNo = $("#PhoneNo").val();
            if (PhoneNo != "") {
                $("#hrPhoneNo").removeClass("hrRequired");
                $("#hrPhoneNo").addClass("hr");
            }
            else {
                $("#hrPhoneNo").removeClass("hr");
                $("#hrPhoneNo").addClass("hrRequired");
            }

        });

        $("#ZipCode").keyup(function (e) {
            var ZipCode = $("#ZipCode").val();
            //alert("ZipCode.length - " + ZipCode.length)
            if (ZipCode != "") {
                if (ZipCode.length < 5) {
                    $("#hrZipCode").removeClass("hr");
                    $("#hrZipCode").addClass("hrRequired");
                }
                else {
                    $("#hrZipCode").removeClass("hrRequired");
                    $("#hrZipCode").addClass("hr");
                }
            }
            else {
                $("#hrZipCode").removeClass("hr");
                $("#hrZipCode").addClass("hrRequired");
            }
        });

        $("#ZipCode").keypress(function (e) {
            //if the letter is not digit then display error and don't type anything
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                return false;
            }
            else {
                return true;
            }
        });

    });
</script>

