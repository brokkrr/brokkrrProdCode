    <script type="text/javascript">

        //11May18
        function openfileDialog() {
            $("#fileToUpload").click();

            $("#fileToUpload").change(function () {
                var FileName = $(this).val();
                var allowedExtensions = ['jpg', 'jpeg', 'jpe', 'png'];

                var ext = FileName.substring(FileName.lastIndexOf('.') + 1);

                if ($.inArray(ext, allowedExtensions) == -1) {
                    //alert("Out");
                    $("#txtUploadDeclaration").val('');
                    $(".showerrormessage").text("Please select valid file (.jpg, .jpeg, .jpe, .png)");
                    $("#ModalError").modal('show');
                }
                else {
                    //alert("In");
                    $("#txtUploadDeclaration").val($(this).val().replace(/C:\\fakepath\\/, ''));

                    var FR = new FileReader();

                    FR.addEventListener("load", function (e) {
                        var base64 = e.target.result;
                        base64 = base64.replace(/^data:image\/(png|jpg|jpeg|gif);base64,/, "").replace(/^data:;base64,/, "").replace(/^data:text\/html;base64,/, "").replace(/^data:application\/pdf;base64,/, "").replace(/^data:application\/vnd.openxmlformats-officedocument.wordprocessingml.document;base64,/, "").replace(/^data:application\/msword;base64,/, "");
                        $("#txtHiddenUploadDeclaration").val(base64);
                    });

                    FR.readAsDataURL(this.files[0]);
                }
            });
        }

        $(document).ready(function () {
            var count = 0;
            

            var Auth = '@ViewBag.User';

            //alert("Authorize - " + Auth)

            if (Auth == "Authorize") {
