            }

            $('#profileImage').click(function () {
                //  alert("hii");
                if (count == 0) {
                    // alert("enter for fadeIn");
                    $('#myDropdown').fadeIn('slow');
                    count++;
                    //  alert("Called for fadeIn");
                }
                else {
                    //  alert("enter for fadeOut");
                    $('#myDropdown').fadeOut('slow');
                    count--;
                    //  alert("Called for fadeOut");
                }

                window.onclick = function (event) {
                    $('#myDropdown').fadeOut('slow');
                    count = 0;
                }

            });

        });

