        $('#city').val(City);
        $('#zipCode').val(ZipCode);
        $('#longitude').val(Longitude);
        $('#latitude').val(Latitude);

        //alert(Longitude + "," + Latitude);
        if (Longitude == '' && Latitude == '') {

            var IntrZipCode = setInterval(
           function () {
               var longitude = $('#longitude').val();
               var latitude = $('#latitude').val();
               //alert(longitude + "," + latitude + "," + ZipCode);

               if (longitude == '' && latitude == '') {
                   $.ajax({
                       url: 'https://maps.googleapis.com/maps/api/geocode/json?address=' + ZipCode + '&key=' + GoogleMapKey + '&sensor=false',
                       success: function (data) {
                           lat = data.results[0].geometry.location.lat;
                           lng = data.results[0].geometry.location.lng;

                           $('#longitude').val(lng);
                           $('#latitude').val(lat);
                           var zip = "", localCity = "", Country = "", State = "";
                           for (var i = 0; i < data.results[0].address_components.length; i++) {

                               for (var b = 0; b < data.results[0].address_components[i].types.length; b++) {
                                   if (data.results[0].address_components[i].types[b] == "postal_code") {
                                       zip = data.results[0].address_components[i].long_name;
                                   }
                                   if (data.results[0].address_components[i].types[b] == "locality") {
                                       localCity = data.results[0].address_components[i].long_name;
                                       $('#city').val(localCity);
                                   }
                                   if (data.results[0].address_components[i].types[b] == "country") {
                                       Country = data.results[0].address_components[i].long_name;
                                       $('#Country').val(Country);
                                   }
                                   if (data.results[0].address_components[i].types[b] == "administrative_area_level_1") {
                                       State = data.results[0].address_components[i].short_name;
                                       //$('#State').val(State);
                                   }
                               }
                           }
                           $('.selected-answer-location').text(ZipCode + ' ' + localCity + ', ' + State + '.');
                       }

                   });
               }
               else {
                   clearInterval(IntrZipCode);
               }
           }, 2000);
        }

        var location = '';
        if (City != '') {
            location = ZipCode + ' ' + City + ', ' + State + '.'
        }
        else {
            location = ZipCode;
        }

        $('.selected-answer-location').text(location);

