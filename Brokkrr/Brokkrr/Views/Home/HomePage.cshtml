<script>
    var count = 0;
    /* When the user clicks on the button,
    toggle between hiding and showing the dropdown content */
    function myFunction() {

        if (count == 0) {
            $('#myDropdown').fadeIn('slow');
            count++;

        }
        else {
            $('#myDropdown').fadeOut('slow');
            count--;
        }
    }
    window.onclick = function (event) {
        $('#myDropdown').fadeOut('slow');
    }
</script>

<script type="text/javascript">
    function CheckValidation() {
        //alert("Called");
        var zipaddress = $("#ZipCode").val();
        //alert(zipaddress);
        if (zipaddress.length >= 5) {
            return true;
        }
        else {
            $("#lblError").text('Enter valid ZipCode.');
            $("#lblError").css("display", "block");
            return false;
        }
    }
    $(document).ready(function () {
        $("#ZipCode").focus();
        var GoogleMapKey = '@ViewBag.GoogleMapKey';

        //called when key is pressed in textbox
        $("#ZipCode").keypress(function (e) {
            //if the letter is not digit then display error and don't type anything
            //if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
            //    return false;
            //}
            //else {
            //    return true;
            //}
            if (e.which == 13) {
                // alert("Called");
                var zipaddress = $("#ZipCode").val();

                if (zipaddress.length >= 5) {
                    $.ajax({
                        url: "/Home/HomePage",
                        type: "POST",
                        success: function (data) {

                        }
                    })
                }
            }
        });


        $("#ZipCode").keyup(function (e) {

            var zipaddress = $("#ZipCode").val();

            if (zipaddress.length >= 5) {
                // alert("Here");
                $.ajax({
                    url: 'https://maps.googleapis.com/maps/api/geocode/json?address=' + zipaddress + '&key=' + GoogleMapKey,
                    success: function (data) {
                        lat = data.results[0].geometry.location.lat;
                        lng = data.results[0].geometry.location.lng;

                        $('#Longitude').val(lng);
                        $('#Latitude').val(lat);

                        for (var i = 0; i < data.results[0].address_components.length; i++) {

                            for (var b = 0; b < data.results[0].address_components[i].types.length; b++) {
                                if (data.results[0].address_components[i].types[b] == "postal_code") {
                                    var zip = data.results[0].address_components[i].long_name;
                                }
                                if (data.results[0].address_components[i].types[b] == "locality") {
                                    localCity = data.results[0].address_components[i].long_name;
                                    $('#City').val(localCity);
                                }
                                if (data.results[0].address_components[i].types[b] == "country") {
                                    Country = data.results[0].address_components[i].long_name;
                                    $('#Country').val(Country);
                                }
                                if (data.results[0].address_components[i].types[b] == "administrative_area_level_1") {
                                    var State = data.results[0].address_components[i].short_name;
                                    $('#State').val(State);
                                }
                            }
                        }
                    }

                });
                $("#lblError").text('');
                $("#lblError").css("display", "none");

                //setTimeout(function () {
                //    if (e.which == 13) {
                //        alert("Called");
                //        var zipaddress = $("#ZipCode").val();

                //        if (zipaddress.length >= 5) {
                //            $.ajax({
                //                url: "/Home/HomePage",
                //                type: "POST",
                //                success: function (data) {

                //                }
                //            })
                //        }
                //    }
                //}, 1000);
            }
        });

        $("#ZipCode").autocomplete({
            source: function (request, response) {
                // alert("Called");
                $.ajax({
                    url: "/Home/GetSearchedZipCodes",
                    type: "POST",
                    dataType: "json",
                    data: { Prefix: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.ZipCode, value: item.ZipCode };
                        }))

                    }
                })
            },
            messages: {
                noResults: "",
                results: function (count) {
                    return count + (count > 1 ? ' results' : ' result ') + ' found';
                }
            }
        });

        var UserId = '@ViewBag.UserId';
        var WeServiceUrl = '@Session["WebServiceURL"]';
        //alert("UserId - " + UserId);

        if (UserId != "") {
            $.ajax({
                type: "POST",
                url: WeServiceUrl,
                data: { UserId: UserId, ActionName: "DoGetUnreadMsgCount" },
                success: function (data) {

                    var obj = JSON.parse(data);
                    var issuccess = obj.IsSuccess;
                    var ErrorMessage = obj.ErrorMessage;
                    var UnreadMsgCnt = obj.UnreadMsgCnt;

                    if (issuccess == true) {
                        //alert("UnreadMsgCnt - " + UnreadMsgCnt);
                        if (UnreadMsgCnt.length > 0) {
                            $.each(UnreadMsgCnt, function (i, res) {
                                if (res.UnreadMsgCnt > 0) {
                                    //alert(res.UnreadMsgCnt);
                                    $("div.badge-warning").text(res.UnreadMsgCnt);
                                }
                                else {
                                    $("div.badge-warning").text(0);
                                }
                            });
                        }
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) { }

            });
        }

        /*Check for unseen video for this user*/
        if (UserId != "") {

            $.ajax({
                type: "POST",
                url: WeServiceUrl,
                data: { UserId: UserId, ActionName: "DoGetUnWatchedVideoCount" },
                success: function (data) {

                    var obj = JSON.parse(data);
                    var issuccess = obj.IsSuccess;
                    var ErrorMessage = obj.ErrorMessage;
                    var VideoDetails = obj.VideoDetails;
                    //alert(VideoDetails)
                    if (issuccess == true) {
                        if (VideoDetails.length > 0) {
                            $.each(VideoDetails, function (i, res) {
                                if (res.Cnt > 0) {
                                    $("#NewVideo").css("display", "block");
                                }
                                else {
                                    $("#NewVideo").css("display", "none");
                                }
                            });
                        }
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) { }

            });
        }

        
    });



</script>

