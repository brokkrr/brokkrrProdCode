<script type="text/javascript">
    function CheckValidation() {
        //alert("Called");
        var zipaddress = $("#ZipCode").val();
        //alert(zipaddress);
        if (zipaddress.length >= 5) {
            //alert("Valid");

            var model = {
                ZipCode: $('#ZipCode').val(),
                City: $('#City').val(),
                Longitude: $('#Longitude').val(),
                Latitude: $('#Latitude').val(),
                Country: $('#Country').val(),
                Company: $('#Company').val(),
                State: $('#State').val(),
            };

            $.ajax({
                url: '@Url.Action("JsonIndex", "Home")',
                data: JSON.stringify(model),
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    if (data.result == 'Success') {

                        window.location.href = '/Insurances/Insurance';
                    }
                    else {

                        window.location.href = '/Home/Index';
                    }
                }
            });
            return true;
        }
        else {
            $("#lblError").text('Enter valid ZipCode.');
            $("#lblError").css("display", "block");
            $("#ZipCode").focus();
            return false;
        }

    }

    $(document).ready(function () {
                
        var GoogleMapKey='@ViewBag.GoogleMapKey';

        //alert("Document Ready - " + GoogleMapKey);

        $("#btnClose").click(function () {
            //alert("Called");
            $("#industry").val("");
            $("#PageBody").css("overflow-y", "scroll");
            $("#ModalSelect").modal('hide');
        });


        $("#industry").val('');

        //$('body').css('opacity', '0.5');

        $("#loading").css("display", "none");

        $(".SelectCompany").change(function () {
            var optionSelected = $("option:selected", this);
            var valueSelected = this.value;
            if (valueSelected == "") {

            }
            else if (valueSelected == "Meineke") {
                //$("#loading").css("display", "block");
                //window.location.href = "/CustomerRegistration/MeinekeCustomerSignUp";
                $("#PageBody").css("overflow-y", "hidden");
                $("#aMeinekeSignUp").css("display", "block");
                $("#aAPSPSignUp").css("display", "none");

                $('#ModalSelect').modal({ backdrop: 'static', keyboard: false });
                $("#ModalSelect").modal('show');
            }
            else if (valueSelected == "APSP") {
                $("#PageBody").css("overflow-y", "hidden");
                $("#aMeinekeSignUp").css("display", "none");
                $("#aAPSPSignUp").css("display", "block");

                $('#ModalSelect').modal({ backdrop: 'static', keyboard: false });
                $("#ModalSelect").modal('show');
                //$("#loading").css("display", "block");
                //window.location.href = "/CustomerRegistration/APSPCustomerSignUp";
            }
        });

        //30April18
        $(".btnrubroker").click(function () {
            $('#ModelOnAreYouBroker').modal({ backdrop: 'static', keyboard: false });
            $("#ModelOnAreYouBroker").modal('show');
        });
        //30April18
        $("#btnCloseAreYouBroker").click(function () {
            $("#ModelOnAreYouBroker").modal('hide');
        });


        //30April18
        $(".alreadyMember").click(function () {
            $('#ModelOnAlreadyMember').modal({ backdrop: 'static', keyboard: false });
            $("#ModelOnAlreadyMember").modal('show');
        });
        //30April18
        $("#btnCloseAlreadyMember").click(function () {
            $("#ModelOnAlreadyMember").modal('hide');
        });

        //called when key is pressed in textbox
        $("#ZipCode").keypress(function (e) {
            //if the letter is not digit then display error and don't type anything
            //if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
            //    return false;
            //}
            //else {
            //    return true;
            //}
            if (e.which == 13) {

                var zipaddress = $("#ZipCode").val();

                if (zipaddress.length >= 5) {

                    if (zipaddress.match(/^\d+$/)) {
                        $.ajax({
                            url: "/Home/HomePage",
                            type: "POST",
                            success: function (data) {

                            }
                        })
                    }
                    else {
                        $("#lblError").text('Enter valid ZipCode.');
                        $("#lblError").css("display", "block");
                        $("#ZipCode").focus();
                    }
                }
            }
        });

        //function GetCity(e)
        $("#ZipCode").keyup(function (e) {
            //alert("Key Up");
            var zipaddress = $("#ZipCode").val();
            //alert("Length" + zipaddress.length);
            if (zipaddress.length >= 5) {
                
                $.ajax({
                    url: 'https://maps.googleapis.com/maps/api/geocode/json?address=' + zipaddress + '&key=' + GoogleMapKey,
                    success: function (data) {
                        lat = data.results[0].geometry.location.lat;
                        lng = data.results[0].geometry.location.lng;

                        $('#Longitude').val(lng);
                        $('#Latitude').val(lat);
                        //alert(JSON.stringify( data.results[0].address_components));
                        for (var i = 0; i < data.results[0].address_components.length; i++) {

                            for (var b = 0; b < data.results[0].address_components[i].types.length; b++) {

                                if (data.results[0].address_components[i].types[b] == "postal_code") {
                                    var zip = data.results[0].address_components[i].long_name;
                                }
                                if (data.results[0].address_components[i].types[b] == "locality") {
                                    localCity = data.results[0].address_components[i].long_name;
                                    $('#City').val(localCity);
                                }
                                if (data.results[0].address_components[i].types[b] == "country") {
                                    Country = data.results[0].address_components[i].long_name;
                                    $('#Country').val(Country);
                                }
                                if (data.results[0].address_components[i].types[b] == "administrative_area_level_1") {
                                    //alert("Data" + data.results[0].address_components[i].types[b] + " Name -" + data.results[0].address_components[i].short_name);
                                    //administrative_area_level_1
                                    var State = data.results[0].address_components[i].short_name;
                                    $('#State').val(State);
                                }
                            }
                        }
                    }

                });
                $("#lblError").text('');
                $("#lblError").css("display", "none");
            }
            //$('#loading').hide();
            //$('.bodycustomer').removeClass('Insurancemainblur');
        });

        $("#ZipCode").blur(function () {
            var zipaddress = $("#ZipCode").val();
            //alert("Length" + zipaddress.length);
            if (zipaddress.length >= 5) {
                //alert("In If" + zipaddress.length);
                $.ajax({
                    url: 'https://maps.googleapis.com/maps/api/geocode/json?address=' + zipaddress + '&key=' + GoogleMapKey,
                    success: function (data) {
                        lat = data.results[0].geometry.location.lat;
                        lng = data.results[0].geometry.location.lng;

                        $('#Longitude').val(lng);
                        $('#Latitude').val(lat);

                        for (var i = 0; i < data.results[0].address_components.length; i++) {

                            for (var b = 0; b < data.results[0].address_components[i].types.length; b++) {
                                if (data.results[0].address_components[i].types[b] == "postal_code") {
                                    var zip = data.results[0].address_components[i].long_name;
                                }
                                if (data.results[0].address_components[i].types[b] == "locality") {
                                    localCity = data.results[0].address_components[i].long_name;
                                    $('#City').val(localCity);
                                }
                                if (data.results[0].address_components[i].types[b] == "country") {
                                    Country = data.results[0].address_components[i].long_name;
                                    $('#Country').val(Country);
                                }
                                if (data.results[0].address_components[i].types[b] == "administrative_area_level_1") {
                                    var State = data.results[0].address_components[i].short_name;
                                    $('#State').val(State);
                                }
                            }
                        }
                    }

                });
            }
        });
        //$('#loading').hide();
        //$('.bodycustomer').removeClass('Insurancemainblur');
    });

</script>


